vertex_program default_GLSL glsl
{
    source default_VS.glsl

    default_params
    {
        param_named_auto u_worldViewProj worldviewproj_matrix
    }
}

vertex_program doubleTexture_P_VS unified
{
    delegate default_GLSL
}



fragment_program doubleTexture_GLSL glsl
{
    source doubleTexture_FS.glsl
}

fragment_program doubleTexture_P_FS unified
{
    delegate doubleTexture_GLSL
}